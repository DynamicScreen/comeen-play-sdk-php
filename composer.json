{
    "name": "martylamoureux/laravel-inertia-vue3-typescript-boilerplate",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "aiken/i18next": "dev-master",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "^0.3.6",
        "laravel/framework": "^8.12",
        "laravel/sanctum": "^2.9",
        "laravel/tinker": "^2.5",
        "socialiteproviders/facebook": "^4.1",
        "socialiteproviders/github": "^4.1",
        "socialiteproviders/gitlab": "^4.1",
        "socialiteproviders/google": "^4.1",
        "socialiteproviders/microsoft": "^4.1",
        "socialiteproviders/zoho": "^4.1",
        "spatie/data-transfer-object": "^3.7",
        "spatie/enum": "^3.9",
        "spatie/laravel-github-webhooks": "^1.1",
        "spatie/laravel-typescript-transformer": "^2.0",
        "symfony/process": "^5.3",
        "symfony/yaml": "^5.2",
        "symplify/git-wrapper": "^9.4",
        "tightenco/ziggy": "^1.4"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./",
            "options": {
                "symlink": true
            }
        }
    ],
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "ziggy":  [
            "@php artisan ziggy:generate ./resources/js/ziggy.generated.js"
        ]
    }
}
